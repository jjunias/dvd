<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Type 별칭 미리 정의하기 -->
	<typeAliases>
		<typeAlias type="com.spring.dvd.users.dto.UsersDto" alias="usersDto"/>
		<typeAlias type="com.spring.dvd.movie.dto.DvdDto" alias="dvdDto"/>
		<typeAlias type="com.spring.dvd.basket.dto.BasketDto" alias="basketDto"/>
		<typeAlias type="com.spring.dvd.rating.dto.RatingDto" alias="ratingDto"/>
		<typeAlias type="com.spring.dvd.qna.dto.QnaDto" alias="qnaDto"/>
		<typeAlias type="com.spring.dvd.rating.dto.RatingRecommendDto" alias="recommendDto"/>
	</typeAliases>

	<!-- sql 문을 정의하고 있는 xml 문서 정보 -->
	<mappers>
		<mapper resource="com/spring/dvd/mybatis/UsersMapper.xml"/>
		<mapper resource="com/spring/dvd/mybatis/DvdMapper.xml"/>
		<mapper resource="com/spring/dvd/mybatis/BasketMapper.xml"/>
		<mapper resource="com/spring/dvd/mybatis/RatingMapper.xml"/>
		<mapper resource="com/spring/dvd/mybatis/QnaMapper.xml"/>
	</mappers>
</configuration>




